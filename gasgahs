section1:NewToggle("Auto Return To Area 2", "", function(lgdm)
lgfp = lgdm
while lgfp do 
task.wait(.1)
if game:GetService("Workspace").ActiveSection:FindFirstChild("Area1") then
hrp.CFrame = CFrame.new(18, 10, -31)
elseif game:GetService("Workspace").ActiveSection:FindFirstChild("Hub") then
hrp.CFrame = CFrame.new(-842, 145, -1)
end
end
end)

section2:NewToggle("Loop Upgrade", "", function(lpl)
lll = lpl
while lll do
task.wait(.1)
if lpm == "PP" then
game:GetService("ReplicatedStorage").Remotes.BuyUpgrade:FireServer("PrestigeUpgrade1")
elseif lpm == "Crystal" then
game:GetService("ReplicatedStorage").Remotes.BuyUpgrade:FireServer("UpgradeCrystal1")
elseif lpm == "Crystal Money" then
game:GetService("ReplicatedStorage").Remotes.BuyUpgrade:FireServer("CrystalMoneyUpgrade1")
else
game:GetService("ReplicatedStorage").Remotes.BuyUpgrade:FireServer("UpgradeMoney"..lpm)
end
end
end)

section2:NewDropdown("Select Upgrade", "", {"1", "2", "3", "4", "PP", "Crystal", "Crystal Money"}, function(lpg)
lpm = lpg
end)
