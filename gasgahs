local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local window = library.CreateLib("Novaz#5792", "BloodTheme")
local main = window:NewTab("Area 1")
local main1 = window:NewTab("Area 2")
local main2 = window:NewTab("Auto Buy")
local section = main:NewSection("Auto Farm Area 1")
local section1 = main1:NewSection("Auto Farm Area 2")
local section2 = main2:NewSection("Auto Buy Upgrades")
local plr = game.Players.LocalPlayer
local hrp = plr.Character.HumanoidRootPart

local part = Instance.new("Part", game.Workspace)
part.Anchored = true
part.Size = Vector3.new(5000,1,5000)
part.CFrame = CFrame.new(383, 5, -71)
part.Transparency = 1

local pat = Instance.new("Part", game.Workspace)
part.Anchored = true
part.Size = Vector3.new(5,1,5)
part.CFrame = CFrame.new(-8, 32, 83)
part.Transparency = 1

local pak = Instance.new("Part", game.Workspace)
part.Anchored = true
part.Size = Vector3.new(5,1,5)
part.CFrame = CFrame.new(465, 21, -73)
part.Transparency = 1

section:NewToggle("Auto Farm 1", "", function(lft)
lfv = lft
while lfv do
task.wait(1)
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, game:GetService("Workspace").ActiveSection.Area1.MoneyPoints.Point1, 0)
wait()
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, game:GetService("Workspace").ActiveSection.Area1.MoneyPoints.Point1, 1)
wait(.1)
firesignal(game:GetService("Players").LocalPlayer.PlayerGui.Main.DefaultInfo.CashIn.MouseButton1Click)
end
end)

section:NewToggle("Auto Farm 2", "", function(lfg)
lfv = lfg
while lfv do
task.wait(.2)
hrp.CFrame = CFrame.new(-8, 34, 83)
wait(1)
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, game:GetService("Workspace").ActiveSection.Area1.MoneyPoints.Point2, 0)
wait()
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, game:GetService("Workspace").ActiveSection.Area1.MoneyPoints.Point2, 1)
wait(.1)
firesignal(game:GetService("Players").LocalPlayer.PlayerGui.Main.DefaultInfo.CashIn.MouseButton1Click)
end
end)

