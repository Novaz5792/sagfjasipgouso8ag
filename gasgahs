section:NewToggle("Auto Return To Area 1", "", function(lfgdm)
lfgfp = lfgdm
while lfgfp do 
task.wait(.1)
if game:GetService("Workspace").ActiveSection:FindFirstChild("Area2") then
hrp.CFrame = CFrame.new(393, 12, -82)
elseif game:GetService("Workspace").ActiveSection:FindFirstChild("Hub") then
hrp.CFrame = CFrame.new(-869, 143, 25)
end
end
end)

